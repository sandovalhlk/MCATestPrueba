<div id="modalReiniciarCheques" class="modal fade in" role="dialog" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-lg modalRedondo">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title panel-title2"> Nuevo inicio de N° de cheque</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    @*<input type="hidden" id="cuentaId" name="cuentaId" value="" />
                    <input type="hidden" id="Cuentabanco" name="Cuentabanco" value="" />*@
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-2 control-label">N° Reinicio<span class="required">*</span></label>
                                <div class="col-md-3">
                                    <input type="text" id="reinicioTxt" name="reinicioTxt" value="" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                @*<label class="col-md-2 control-label">CREDITO<span class="required">*</span></label>*@
                                <div class="col-md-12">
                                    <label> Justificacion </label>
                                    <textarea id="justificacion" class="form-control"></textarea>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" id="btnSaveReiniciarNum" class="btn btn-default">Guardar</button>
                    <button type="button" class="btn btn-inverse" data-dismiss="modal">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
</div>
