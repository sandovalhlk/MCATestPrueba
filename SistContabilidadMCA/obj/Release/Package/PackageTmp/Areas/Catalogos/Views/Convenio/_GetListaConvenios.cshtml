@if (Model != null)
{

    <div class="table-responsive m-t-40" style="margin-top:10px">
        <table id="myTable" class="display nowrap table table-hover table-bordered stylish-table" cellspacing="0" width="100%" style="font-size:12px">
            <thead style="color:#000;">
                <tr>
                    <th style="text-align:center"></th>
                    <th style="text-align:center">Convenios</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr style="color:#263238;">                       
                        <td style="text-align:center" width="60"><a class="btn btn-info btn-circle btn-sm"  title="Editar Convenio" data-toggle='tooltip' onclick="return fnSetEditData(@item.convenioId)"><i class="mdi mdi-reorder-horizontal" style="color:#fff;"></i></a></td>
                        <td style="text-align:center">@item.convenio</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

}
