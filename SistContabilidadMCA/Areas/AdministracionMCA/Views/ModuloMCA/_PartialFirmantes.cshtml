<div id="modalAddFirmantes" class="modal fade in" role="dialog" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content ">
            <div class="modal-header">
                @*<button type="button" class="close" data-dismiss="modal">×</button>*@
                <h4 class="modal-title panel-title2"> Registro de Firmantes</h4>
            </div>
            <div class="modal-body ">
                @*<div class="content-fluid">*@
                    <div class="row">
                        <div class="col-md-12">
                            <input id="usuarioId" type="hidden" value="" />
                            <div class="form-group">
                                <div class="row">
                                    <label class="col-md-2 control-label">Cédula <span class="required">*</span></label>
                                    <div class="col-md-3" style="padding-left: 0px;">
                                        <input type="text" id="cedula" value="" class="form-control cedula" />

                                    </div>

                                    <div class="col-md-1" style="padding-left: 0px;padding-right: 0px;">
                                        <input type="button" class="btn btn-info" id="btnCargarCedula" value="Cargar" style="width:100%" />
                                    </div>
                                    <label class="col-md-2 control-label">Nombres<span class="required">*</span></label>
                                    <div class="col-md-4">
                                        <input type="text" id="nombres" value="" disabled="disabled" class="form-control" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <label class="col-md-2 control-label">Apellidos<span class="required">*</span></label>
                                    <div class="col-md-4" style="padding-right: 15px;padding-left: 0px;">
                                        <input type="text" id="apellidos" value="" disabled="disabled" class="form-control" />
                                    </div>
                                    <label class="col-md-2 control-label">Tipo Firmante<span class="required">*</span></label>
                                    <div class="col-md-4" style="padding-left: 0px;padding-right: 15px;">
                                        @Html.DropDownList("tipoUsuarioId", ViewBag.tipoPersonas as SelectList, "***Seleccione una opcion***", htmlAttributes: new { @class = "form-control", @style = "width:100%" })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="modal-footer">
                        <button type="button" id="btnInsertarFirmantes" class="btn btn-primary">Guardar</button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
                    </div>
                </div>
        </div>

    </div>
</div>

